{
    "name": "ariaieboy/filament-jalali-datetimepicker",
    "description": "This is my package filament-jalali-datetimepicker",
    "keywords": [
        "ariaieboy",
        "laravel",
        "filament-jalali-datetimepicker"
    ],
    "homepage": "https://github.com/ariaieboy/filament-jalali-datetimepicker",
    "license": "MIT",
    "authors": [
        {
            "name": "ariaieboy",
            "email": "smr.seddighy@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.0|^9.0",
        "filament/filament": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Ariaieboy\\FilamentJalaliDatetimepicker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ariaieboy\\FilamentJalaliDatetimepicker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ariaieboy\\FilamentJalaliDatetimepicker\\FilamentJalaliDatetimepickerServiceProvider"
            ],
            "aliases": {
                "FilamentJalaliDatetimepicker": "Ariaieboy\\FilamentJalaliDatetimepicker\\Facades\\FilamentJalaliDatetimepicker"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
